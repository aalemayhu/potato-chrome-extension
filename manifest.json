{
  "name": "Potato",
  "description": "The potato extension - plants potatoes around the web",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "action": {
    "default_pop": "potato.html",
    "default_icon": "potato_extensions.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "MacCtrl+Shift+P"
      },
      "description": "Create potato"
    }
  }
}
